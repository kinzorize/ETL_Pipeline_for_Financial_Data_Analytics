{% set INPUT_BUCKET_NAME = env['project'] + '-etl-input-2023' %}
{% set RESOURCES_BUCKET_NAME = env['project'] + '-etl-resources-2023' %}

resources:
- type: gcp-types/storage-v1:buckets
  name: {{ INPUT_BUCKET_NAME }}
  properties:
    predefinedAcl: projectPrivate
    projection: full
    location: US
    storageClass: STANDARD

- type: gcp-types/storage-v1:buckets
  name: {{ RESOURCES_BUCKET_NAME }}
  properties:
    predefinedAcl: projectPrivate
    projection: full
    location: US
    storageClass: STANDARD    